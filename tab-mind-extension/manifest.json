{
  "manifest_version": 3,
  "name": "Tab Mind â€“ AI Tab Organizer",
  "version": "1.0.0",
  "description": "AI-powered tab organizer that categorizes, saves, and manages your browser tabs intelligently",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "background"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Tab Mind",
    "default_icon": {
      "16": "icons/icon16.svg",
      "32": "icons/icon32.svg",
      "48": "icons/icon48.svg",
      "128": "icons/icon128.svg"
    }
  },
  "icons": {
    "16": "icons/icon16.svg",
    "32": "icons/icon32.svg",
    "48": "icons/icon48.svg",
    "128": "icons/icon128.svg"
  },
  "web_accessible_resources": [
    {
      "resources": ["suspended.html"],
      "matches": ["<all_urls>"]
    }
  ]
}